<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.hqlqueries.entities">
	<class name="Journey" table="journey">
		<id name="journeyNo" column="journey_no">
			<generator class="increment" />
		</id>
		<property name="source" />
		<property name="destination" />
		<property name="journeyDate" column="journey_date" />
		<property name="commuteMode" column="commute_mode" />
		<property name="hours" />
		<property name="amount" />
	</class>
</hibernate-mapping>